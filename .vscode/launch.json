            "servertype": "jlink",
            "serverpath": "${config:jlink_server_path}",
            "device": "STM32F413VG",
            "serialNumber": "${config:jlink_serial_number}",
            "interface": "swd"
        },
        {
            "name": "游띲游리 | m0 STLink launch",
