                "group": "Local"
            },
            "type": "cortex-debug",
            "request": "launch",
            "executable": "${workspaceFolder}/build/fw_m0.elf",
            "svdFile": "${workspaceFolder}/platform/m0/core/stm32g431.svd",
            "gdbPath": "${config:gdb_path}",
            "showDevDebugOutput": "raw",
            "servertype": "stlink",
            "interface": "swd"
        },
        {
            "name": "游리游릭 | m0 BMP launch",
            "presentation": {
                "group": "Local"
            },
            "type": "cortex-debug",
            "request": "launch",
            "executable": "${workspaceFolder}/build/fw_m0.elf",
