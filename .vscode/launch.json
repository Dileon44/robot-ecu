{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "游리游릭 | m3 STLink launch",
            "presentation": {
                "group": "Local"
            },
            "type": "cortex-debug",
            "request": "launch",
            "executable": "${workspaceFolder}/build/app.elf",
            "svdFile": "${workspaceFolder}/platform/m3/core/stm32f413.svd",
            "gdbPath": "${config:gdb_path}",
            "showDevDebugOutput": "raw",
            "servertype": "stlink",
            "interface": "swd"
        },
        {
            "name": "游리游릭 | m3 OpenOCD-STLink launch",
            "presentation": {
                "group": "Local"
            },
            "type": "cortex-debug",
            "request": "launch",
            "executable": "${workspaceFolder}/build/app.elf",
            "svdFile": "${workspaceFolder}/platform/m3/core/stm32f413.svd",
            "gdbPath": "${config:gdb_path}",
            "showDevDebugOutput": "raw",
            "servertype": "openocd",
            "serverpath": "${config:openocd_path}",
            "interface": "swd",
            "configFiles": [
                "${config:openocd_scripts_path}/interface/stlink.cfg",
                "${config:openocd_scripts_path}/target/stm32f4x.cfg"
            ],
            "preLaunchCommands": [
                "monitor init",
                "monitor reset init",
                "monitor flash write_image debug/fw_m3.elf"
            ]
        },
        {
            "name": "游리游릭 | m3 BMP launch",
            "presentation": {
                "group": "Local"
            },
            "type": "cortex-debug",
            "request": "launch",
            "executable": "${workspaceFolder}/build/app.elf",
            "svdFile": "${workspaceFolder}/platform/m3/core/stm32f413.svd",
            "gdbPath": "${config:gdb_path}",
            "showDevDebugOutput": "raw",
            "servertype": "bmp",
            "BMPGDBSerialPort": "${config:bmp_serial_port}"
        },
        {
            "name": "游리游릭 | m3 BMP attach",
            "presentation": {
                "group": "Local"
            },
            "type": "cortex-debug",
            "request": "attach",
            "executable": "${workspaceFolder}/build/app.elf",
            "svdFile": "${workspaceFolder}/platform/m3/core/stm32f413.svd",
            "gdbPath": "${config:gdb_path}",
            "showDevDebugOutput": "raw",
            "servertype": "bmp",
            "BMPGDBSerialPort": "${config:bmp_serial_port}"
        },
        {
            "name": "游리游릭 | m3 JLink launch",
            "presentation": {
                "group": "Local"
            },
            "type": "cortex-debug",
            "request": "launch",
            "executable": "${workspaceFolder}/build/app.elf",
            "svdFile": "${workspaceFolder}/platform/m3/core/stm32f413.svd",
            "gdbPath": "${config:gdb_path}",
            "showDevDebugOutput": "raw",
            "servertype": "jlink",
            "serverpath": "${config:jlink_server_path}",
            "device": "STM32F413VG",
            "serialNumber": "${config:jlink_serial_number}",
            "interface": "swd"
        },
        {
            "name": "游리游릭 | m3 JLink attach",
            "presentation": {
                "group": "Local"
            },
            "type": "cortex-debug",
            "request": "attach",
            "executable": "${workspaceFolder}/build/app.elf",
            "svdFile": "${workspaceFolder}/platform/m3/core/stm32f413.svd",
            "gdbPath": "${config:gdb_path}",
