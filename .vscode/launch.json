        {
            "name": "游리游릭 | m0 OpenOCD-CMSIS-Dap launch",
            "presentation": {
                "group": "Local"
            },
            "type": "cortex-debug",
            "request": "launch",
            "executable": "${workspaceFolder}/build/fw_m0.elf",
            "svdFile": "${workspaceFolder}/platform/m0/stm32g431.svd",
            "gdbPath": "${config:gdb_path}",
            "showDevDebugOutput": "raw",
            "servertype": "openocd",
            "serverpath": "${config:openocd_path}",
            "interface": "swd",
            "configFiles": [
                "${workspaceFolder}/cmsis-dap.cfg",
                "${workspaceFolder}/platform/m0/stm32g4x.cfg"
            ],
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 10
            }
        }
